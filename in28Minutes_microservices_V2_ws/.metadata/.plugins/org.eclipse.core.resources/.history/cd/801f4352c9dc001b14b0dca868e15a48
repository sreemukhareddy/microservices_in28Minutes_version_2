package com.in28minutes.microservices;
@Data
@NoArgsConstructor
@AllArgsConstructor
@Entity
public class ExchangeValue {


	@Id
	private Long id;
	
	@Column(name = "currency_from")
	private String from;
	
	@Column(name = "currency_to")
	private String to;
	private BigDecimal conversionMultiple;
	private Integer port;
}